{
  "name": "bpftrace-vscode",
  "displayName": "bpftrace",
  "description": "Extension for bpftrace",
  "version": "0.1.2",
  "publisher": "e1568e4d-f695-62fe-827b-050ee0dfd8cf",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bpfdeploy-io/bpftrace-vscode"
  },
  "bugs": {
    "url": "https://github.com/bpfdeploy-io/bpftrace-vscode/issues"
  },
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "bpftrace",
    "bpf",
    "ebpf",
    "linux",
    "dtrace",
    "awk",
    "tracing"
  ],
  "contributes": {
    "languages": [
      {
        "id": "bpftrace",
        "aliases": [
          "bpftrace"
        ],
        "extensions": [
          ".bt"
        ]
      }
    ],
    "grammars": [
      {
        "language": "bpftrace",
        "path": "./bpftrace.tmLanguage.json",
        "scopeName": "source.bpftrace",
        "embeddedLanguages": {
          "meta.embedded.block.c": "c"
        }
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -watch -p ./",
    "pkg": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.39.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.13.0",
    "typescript": "^4.7.2",
    "vsce": "^2.10.0"
  }
}
